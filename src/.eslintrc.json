{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "experimentalObjectRestSpread": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  }
}